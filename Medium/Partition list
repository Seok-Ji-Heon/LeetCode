# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def partition(self, head: Optional[ListNode], x: int) -> Optional[ListNode]:
        
        curr = head
        nums = []
        while curr:
            nums.append(curr.val)
            curr = curr.next
        
        small_num, large_num = [], []
        
        for i in range(len(nums)):
            
            if nums[i]<x:
                small_num.append(nums[i])
            else:
                large_num.append(nums[i])
        
        nums = small_num + large_num
        
        head = ListNode(0)
        
        curr = head
        
        for num in nums:
            curr.next = ListNode(num)
            curr = curr.next
        
        return head.next

class Solution:
    def breakPalindrome(self, palindrome: str) -> str:
        length = len(palindrome)
        
        if length==1:
            return ''
        
        results = []
        
        for i in range(len(palindrome)):
            if palindrome[i]=='a':
                temp = palindrome[:i] + 'b' + palindrome[i+1:]
            else:
                temp = palindrome[:i] + 'a' + palindrome[i+1:]
            
            if temp!=temp[::-1]:
                results.append(temp)
        
        return min(results)

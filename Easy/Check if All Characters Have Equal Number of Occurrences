class Solution:
    def areOccurrencesEqual(self, s: str) -> bool:
        h = {}
        for char in s:
            
            if char not in h:
                h[char]=1
            
            else:
                h[char]+=1
        
        cnt = 0
        
        for value in h.values():
            
            if cnt ==0:
                cnt = value
            
            if cnt!=value:
                return False
        
        return True

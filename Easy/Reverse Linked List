# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def reverseList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        nums = []
        while head!=None:
            nums.append(head.val)
            head = head.next
        
        head = ListNode(0)
        curr = head
        
        while nums!=[]:
            curr.next =ListNode(nums.pop())
            
            curr = curr.next
        
        return head.next

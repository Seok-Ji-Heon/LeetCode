class Solution:
    def countGoodRectangles(self, rectangles: List[List[int]]) -> int:
        
        mx = 0
        
        for rectangle in rectangles:
            
            if rectangle[0]>rectangle[1]:
                rectangle[0], rectangle[1] = rectangle[1], rectangle[0]
            
            if mx<rectangle[0]:
                mx = rectangle[0]
                cnt = 1
            
            elif mx==rectangle[0]:
                cnt+=1
        
        return cnt

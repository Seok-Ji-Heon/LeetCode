class Solution:
    def findTheDifference(self, s: str, t: str) -> str:
        
        s = list(s)
        t = list(t)
        
        
        for i in range(len(t)-1):
            if t[i] in s:
                del s[s.index(t[i])]
        
            else:
                return t[i]
        
        return t[-1]

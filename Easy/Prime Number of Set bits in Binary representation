class Solution:
    def countPrimeSetBits(self, left: int, right: int) -> int:
        
        def prime(num : int):
            if num==1:
                return False
            if num==2:
                return True
            
            if num%2==0:
                return False
            
            for i in range(3,int(math.sqrt(num))+1):
                
                if num%i ==0:
                    return False
            
            return True
        
        cnt = 0
        for i in range(left, right+1):
            
            binary = bin(i)[2:]
            
            bit = binary.count('1')
            
            if prime(bit):
                cnt+=1
                
        
        return cnt

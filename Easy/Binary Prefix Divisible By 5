class Solution:
    def prefixesDivBy5(self, A: List[int]) -> List[bool]:
        
        num = ''
        
        nums = []
        
        for i in A:
            num = num + str(i)
            nums.append(int(num,2))
        
        output = []
        
        for i in nums:
            if i%5==0:
                output.append(True)
            else:
                output.append(False)
        
        return output
        
        
 class Solution:
    def prefixesDivBy5(self, A: List[int]) -> List[bool]:
        
        num = 0
        
        nums = []
        
        for i in A:
            num = 2*num + 2*i
            if num%5==0:
                nums.append(True)
            else:
                nums.append(False)
        return nums

class Solution:
    def countTriples(self, n: int) -> int:
        
        squares = []
        
        for i in range(1,n+1):
            squares.append(i**2)
        cnt = 0
        for i in range(len(squares)-1):
            
            for num2 in squares:
                
                if (squares[i]+num2) in squares:
                    cnt+=1
        
        return cnt

class Solution:
    def xorOperation(self, n: int, start: int) -> int:
        
        nums = []
        
        for i in range(n):
            nums.append(start+2*i)
        
        output = 0
        
        for i in nums:
            if output ==0:
                output = i
            
            else:
                output = output ^ i
        
        return output

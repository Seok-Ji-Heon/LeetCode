class Solution:
    def findContentChildren(self, g: List[int], s: List[int]) -> int:
        
        g.sort()
        s.sort()
        
        idx1, idx2 =0, 0 
        output=0
        while idx1<len(g) and idx2<len(s):
            
            if g[idx1]<=s[idx2]:
                idx1+=1
                idx2+=1
                output+=1
            
            else:
                idx2+=1
        
        return output

class Solution:
    def findMaxConsecutiveOnes(self, nums: List[int]) -> int:
        mx, cnt = 0, 0
        for i in range(len(nums)):
            if i==0 and nums[i]==1:
                cnt+=1
            
            elif nums[i]==nums[i-1] and nums[i]==1:
                cnt+=1
            elif nums[i-1]==0 and nums[i]==1:
                cnt = 1
            if cnt>mx:
                mx = cnt
        
        return mx

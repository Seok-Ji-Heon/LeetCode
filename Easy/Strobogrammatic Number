class Solution:
    def isStrobogrammatic(self, num: str) -> bool:
        
        h = {'1':'1','6':'9','8':'8','9':'6','0':'0'}
        
        output = ''
        
        for i in range(len(num)-1,-1,-1):
            
            if num[i] not in h:
                return False
            
            output += h[num[i]]
            
        
        return output==num

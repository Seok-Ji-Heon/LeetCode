class Solution:
    def check(self, nums: List[int]) -> bool:
        rotate = False
        for i in range(1,len(nums)):
            
            if nums[i-1]>nums[i]:
                nums_copy = nums[i:] + nums[:i]
                rotate = True
        
        if rotate==False:
            
            nums_copy = nums[::]
            
            
        return nums_copy==list(sorted(nums))
